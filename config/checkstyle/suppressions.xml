    <suppress files="[\\/]com[\\/]bitzi[\\/]" checks=".*"/>
    <suppress files="[\\/]com[\\/]sun[\\/]" checks=".*"/>
    <suppress files="[\\/]com[\\/]twmacinta[\\/]" checks=".*"/>
    <suppress files="[\\/]gnu[\\/]crypto[\\/]" checks=".*"/>
    <suppress files="[\\/]jonelo[\\/]jacksum[\\/]" checks=".*"/>
</suppressions>
